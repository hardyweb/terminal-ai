# Gopass Password Manager Integration
# Enable gopass to retrieve secrets from password manager
USE_GOPASS=false

# OpenRouter (Primary - Priority 1)
# Option 1: Direct API key
OPENROUTER_API_KEY=your_openrouter_api_key_here
# Option 2: Use gopass (format: gopass:path/to/secret)
# OPENROUTER_API_KEY=gopass:terminal-ai/openrouter_api_key
OPENROUTER_ENDPOINT=https://openrouter.ai/api/v1/chat/completions
OPENROUTER_MODEL=meta-llama/llama-3.2-3b-instruct:free

# OpenRouter Custom BYOK (Priority 0 - Highest Priority)
# Bring Your Own Key for custom OpenRouter instance
OPENROUTER_CUSTOM_API_KEY=your_custom_openrouter_key_here
# OPENROUTER_CUSTOM_API_KEY=gopass:terminal-ai/openrouter_custom_key
OPENROUTER_CUSTOM_ENDPOINT=https://your-custom-instance.com/api/v1/chat/completions
OPENROUTER_CUSTOM_MODEL=your-custom-model

# Gemini (Secondary - Priority 2)
# Option 1: Direct API key
GEMINI_API_KEY=your_gemini_api_key_here
# Option 2: Use gopass (format: gopass:path/to/secret)
# GEMINI_API_KEY=gopass:terminal-ai/gemini_api_key
GEMINI_ENDPOINT=https://generativelanguage.googleapis.com/v1beta/models/gemini-2.0-flash:generateContent
GEMINI_MODEL=gemini-2.0-flash

# Groq (Tertiary - Priority 3)
# Option 1: Direct API key
GROQ_API_KEY=your_groq_api_key_here
# Option 2: Use gopass (format: gopass:path/to/secret)
# GROQ_API_KEY=gopass:terminal-ai/groq_api_key
GROQ_ENDPOINT=https://api.groq.com/openai/v1/chat/completions
GROQ_MODEL=llama-3.3-70b-versatile

# Web Server Configuration
WEB_HOST=0.0.0.0
WEB_PORT=8181
